<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:AddressablesGUI="UnityEditor.AddressableAssets.GUIElements" xmlns:editor="UnityEditor.UIElements" xmlns:MemoryProfiler="Unity.MemoryProfiler.Editor" xmlns:engine="UnityEngine.UIElements" xmlns:inventory="MyCode.UI.Inventory" xmlns="UnityEditor.Overlays" elementFormDefault="qualified" targetNamespace="MyCode.UI.MainMenu" xmlns:xs="http://www.w3.org/2001/XMLSchema">
